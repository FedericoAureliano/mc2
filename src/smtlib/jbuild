(jbuild_version 1)

; main binary
(library
  ((name mc2_smtlib)
   (public_name mc2.smtlib)
   (optional) ; only if deps present
   (libraries (containers mc2.core))
   (flags (:standard -w +a-4-42-44-48-50-58-32-60@8))
   (ocamlopt_flags (:standard -O3
                    -unbox-closures -unbox-closures-factor 20))
  ))

